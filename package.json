    "concurrently": "^3.5.1",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.2",
    "express-hbs": "^1.1.0",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.1",
    "faker": "^4.1.0",
    "mysql": "^2.16.0",
    "nodeadmin": "^0.1.2",
    "password-hash": "^1.2.2",
    "react-spring": "^8.0.19",
